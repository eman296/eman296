pragma solidity ^ 0.5.0;

contract Mycontract
{
    enum State {Wating,Ready,Active}
    State public state;
    constructor () public
    {
        state = State.Wating;
    }
    function Active() public
    {
        state=State.Active;
    }
    function isActivate() public view returns (bool)
    {
       return state == State.Active;
    }
}
