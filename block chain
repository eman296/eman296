//web 3.0 version is used
//smart contract
pragma solidity >=0.4.16 <0.9.0;//include version 
contract Firstcode//a type of some function
{   //declare state variable newx
    uint newx;//uint fix value 256 bits
    //setter function
     function set(uint x) public//scope resulation operator
     {
         newx = x;
     }
     //getter
    function get() public view returns(uint)
    {
        return newx;
    }
}
